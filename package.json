{
    "name": "vscode-cult",
    "displayName": "vscode-cult",
    "description": "Cult language support for VSCode",
    "keywords": [
        "syntax",
        "Cult"
    ],
    "version": "0.1.0",
    "publisher": "cgrinker",
    "icon": "images/lambda.png",
    "main": "./out/src/cultMain",
    "license": "MIT",
    "author": {
        "name": "Clark Rinker"
    },
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:cult"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cgrinker/vscode-cult.git"
    },
    "bugs": {
        "url": "https://github.com/cgrinker/vscode-cult/issues"
    },
    "homepage": "https://github.com/cgrinker/vscode-cult",
    "contributes": {
        "languages": [
            {
                "id": "cult",
                "aliases": [
                    "Cult",
                    "cult"
                ],
                "extensions": [
                    ".cult",
                    ".cul"
                ],
                "configuration": "./cult.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cult",
                "scopeName": "source.cult",
                "path": "./syntaxes/cult.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "cult",
                "path": "./snippets/cult.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
