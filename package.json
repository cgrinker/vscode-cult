{
    "name": "vscode-cult",
    "displayName": "vscode-cult",
    "description": "Cult language support for VSCode",
    "keywords": [
        "syntax",
        "Cult"
    ],
    "version": "0.4.0",
    "publisher": "clarkrinker",
    "icon": "images/lambda.png",
    "main": "./src/cultMain",
    "license": "MIT",
    "author": {
        "name": "Clark Rinker"
    },
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:cult"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cgrinker/vscode-cult.git"
    },
    "bugs": {
        "url": "https://github.com/cgrinker/vscode-cult/issues"
    },
    "homepage": "https://github.com/cgrinker/vscode-cult",
    "contributes": {
        "languages": [
            {
                "id": "cult",
                "aliases": [
                    "Cult",
                    "cult"
                ],
                "extensions": [
                    ".cult",
                    ".cul"
                ],
                "configuration": "./cult.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cult",
                "scopeName": "source.cult",
                "path": "./syntaxes/cult.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "cult",
                "path": "./snippets/cult.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.1.17"
    },
    "dependencies": {
        "vscode-languageclient": "4.0.0"
    }
}
